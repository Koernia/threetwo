{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "./src/"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": [
    "better-docs/component",
    "better-docs/category",
    "plugins/markdown",
    "node_modules/better-docs/typescript"
  ],
  "templates": {
    "better-docs": {
      "name": "ThreeTwo UI components"
    }
  },
  "opts": {
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "encoding": "utf8",
    "verbose": true,
    "template": "node_modules/better-docs"
  }
}